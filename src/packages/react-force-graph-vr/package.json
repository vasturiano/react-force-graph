{
  "name": "react-force-graph-vr",
  "version": "1.1.0",
  "description": "React component for VR force directed graphs",
  "unpkg": "dist/react-force-graph-vr.min.js",
  "main": "dist/react-force-graph-vr.common.js",
  "module": "dist/react-force-graph-vr.module.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vasturiano/react-force-graph.git"
  },
  "homepage": "https://github.com/vasturiano/react-force-graph",
  "keywords": [
    "react",
    "force",
    "graph",
    "vr",
    "a-frame",
    "webgl"
  ],
  "author": {
    "name": "Vasco Asturiano",
    "url": "http://bl.ocks.org/vasturiano"
  },
  "bugs": {
    "url": "https://github.com/vasturiano/react-force-graph/issues"
  },
  "scripts": {
    "build": "rimraf dist && rollup -c",
    "watch": "rollup -c -w",
    "minify": "uglifyjs dist/react-force-graph-vr.js -o dist/react-force-graph-vr.min.js -c -m --comments '/Version/'",
    "build-module": "rollup -c rollup.config.module.js",
    "prepublishOnly": "npm run build && npm run minify && npm run build-module"
  },
  "dependencies": {
    "3d-force-graph-vr": "^1.21.0",
    "prop-types": "^15.6.2",
    "react-kapsule": "^1.4.1"
  },
  "peerDependencies": {
    "react": "*"
  },
  "devDependencies": {
    "rimraf": "^2.6.2",
    "rollup": "^0.66.6",
    "uglify-es": "^3.3.9"
  }
}
